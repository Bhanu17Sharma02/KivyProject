<Login>:
    name:"log"
    md_bg_color:(1,1,1,1)
    MDLabel:
        text:"Smart Jammu & Kashmir"
        font_style:"H6"
        underline:True
        size_hint:1,1
        pos_hint:{"center_x":0.6,"center_y":0.9}
        theme_text_color:"Custom"
        text_color:(0,1,0,1)
    MDTextField:
        hint_text:"Username"
        icon_right:"account"
        icon_right_color:app.theme_cls.primary_color
        size_hint_x:None
        width:220
        font_size:20
        pos_hint:{'center_x':0.5,'center_y':0.6}
        color_active:[0,1,0,1]
        helper_text:"Enter username"
        helper_text_mode:"on_focus"

    MDTextField:
        hint_text:"password"
        icon_right:"eye-off"
        icon_right_color:app.theme_cls.primary_color
        size_hint_x:None
        width:220
        font_size:20
        pos_hint:{'center_x':0.5,'center_y':0.5}
        color_active:[0,1,0,1]
        helper_text:"Enter password"
        helper_text_mode:"on_focus"
        password:True

    MDRoundFlatButton:
        text:"Log in"
        pos_hint:{"center_x":.5,"center_y":.4}
        font_size:15
        on_press:
            root.manager.current="option"
			root.manager.transition.direction = "left"

    MDRoundFlatButton:
        text:"Forget Password"
        pos_hint:{"center_x":.5,"center_y":.3}
        font_size:11
        on_press:
            root.on_forget()
			root.manager.transition.direction = "right"

    MDRoundFlatButton:
        text:"New User Registration"
        pos_hint:{"center_x":.5,"center_y":.2}
        font_size:10
        on_press:
            root.manager.transition.direction = "right"

